    .)
	;;
    *)
	{ case $subdir_dir in
	[\\/]* | ?:[\\/]* ) total_dir=;;
	*)                      total_dir=.;;
	esac
	temp=$subdir_dir
	for dir_part in `IFS='/\\'; set X $temp; shift; echo "$@"`; do
	    case $dir_part in
	    # Skip DOS drivespec
	    ?:) total_dir=$dir_part ;;
	    *)  total_dir=$total_dir/$dir_part
		test -d "$total_dir" ||
		mkdir "$total_dir" ||
		as_fn_error $? "cannot create $subdir_dir" "$LINENO" 5
		;;
	    esac
	done; }

	if test -d ./$subdir_dir; then :;
	else
	    as_fn_error $? "cannot create \`pwd\`/$subdir_dir" "$LINENO" 5
	fi
	;;
    esac

