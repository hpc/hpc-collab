# Some helper script functions.  Unfortunately, we cannot use  kinds
# of arugments here because of the m4 substitution.  So we have to set
# special variable names before invoking the function.  :-\

pmix_show_title() {
  cat <<EOF

============================================================================
== ${1}
============================================================================
EOF

# 1 is the message
# 2 is whether to put a prefix or not
if test -n "1"; then
    echo "configure:3257: === ${1}" >&5
else
    echo === ${1} >&5
fi
}


pmix_show_subtitle() {
  cat <<EOF

*** ${1}
EOF

# 1 is the message
# 2 is whether to put a prefix or not
if test -n "1"; then
    echo "configure:3273: *** ${1}" >&5
else
    echo *** ${1} >&5
fi
}


pmix_show_subsubtitle() {
  cat <<EOF

+++ ${1}
EOF

# 1 is the message
# 2 is whether to put a prefix or not
if test -n "1"; then
    echo "configure:3289: +++ ${1}" >&5
else
    echo +++ ${1} >&5
fi
}

pmix_show_subsubsubtitle() {
  cat <<EOF

--- ${1}
EOF

# 1 is the message
# 2 is whether to put a prefix or not
if test -n "1"; then
    echo "configure:3304: --- ${1}" >&5
else
    echo --- ${1} >&5
fi
}

pmix_show_verbose() {
  if test "$V" = "1"; then
      cat <<EOF
+++ VERBOSE: ${1}
EOF

# 1 is the message
# 2 is whether to put a prefix or not
if test -n "1"; then
    echo "configure:3319: --- ${1}" >&5
else
    echo --- ${1} >&5
fi
  fi
}

